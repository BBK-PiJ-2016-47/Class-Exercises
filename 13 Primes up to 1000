//Print all primes up to 1000

int i = 2
println "all prime numbers up to 1000"

for (int num = 0;num < 1000;num++) {
  while (i < 10) {
    if ( i == num) {
      i++
    } else {
      int a = num % i
        if (a != 0) {
          println num
          i++
        } else {
          i++
        } 
      } 
    } 
}
